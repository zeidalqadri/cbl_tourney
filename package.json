{
  "name": "tourney",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "build:cloudflare": "next build && npm run build:worker",
    "build:worker": "wrangler deploy --dry-run --outdir dist --config wrangler.api.toml",
    "deploy:pages": "wrangler pages deploy out",
    "deploy:worker": "wrangler deploy --config wrangler.api.toml",
    "deploy:all": "npm run build:cloudflare && npm run deploy:pages && npm run deploy:worker",
    "cf:dev": "next dev & wrangler dev --config wrangler.api.toml --port 8787",
    "sync-tournament": "node sync-tournament-data.js",
    "validate-tournament": "node validate-tournament-data.js",
    "check-db": "node check-database.js"
  },
  "dependencies": {
    "@nodelib/fs.walk": "^3.0.1",
    "@supabase/ssr": "^0.5.2",
    "@supabase/supabase-js": "^2.53.0",
    "@types/html2canvas": "^0.5.35",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.4.20",
    "dotenv": "^17.2.1",
    "framer-motion": "^12.23.12",
    "html2canvas": "^1.4.1",
    "lucide-react": "^0.478.0",
    "motion-utils": "^12.23.6",
    "next": "14.2.31",
    "postcss": "^8",
    "react": "^18",
    "react-dom": "^18",
    "react-swipeable": "^7.0.2",
    "tailwindcss": "^3.3.0",
    "typescript": "^5"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "^1.13.13",
    "eslint": "^9.17.0",
    "eslint-config-next": "15.1.3"
  },
  "overrides": {
    "glob": "^10.4.5",
    "rimraf": "^6.0.1",
    "@humanwhocodes/config-array": "npm:@eslint/config-array@^0.19.1",
    "@humanwhocodes/object-schema": "npm:@eslint/object-schema@^2.1.5"
  }
}