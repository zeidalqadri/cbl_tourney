{
  "name": "tourney",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "NODE_OPTIONS='--max-old-space-size=4096' next build",
    "build:fast": "NODE_OPTIONS='--max-old-space-size=4096' NEXT_TELEMETRY_DISABLED=1 next build",
    "pages:build": "NODE_OPTIONS='--max-old-space-size=4096' next build",
    "start": "next start",
    "lint": "next lint",
    "deploy": "npm run build:fast && npx wrangler pages deploy out --project-name=cbltour",
    "clean": "rm -rf .next out node_modules/.cache",
    "sync-tournament": "node sync-tournament-data.js",
    "validate-tournament": "node validate-tournament-data.js",
    "check-db": "node check-database.js"
  },
  "dependencies": {
    "@nodelib/fs.walk": "^3.0.1",
    "@supabase/ssr": "^0.5.2",
    "@supabase/supabase-js": "^2.53.0",
    "@types/html2canvas": "^0.5.35",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.4.20",
    "critters": "^0.0.23",
    "dotenv": "^17.2.1",
    "framer-motion": "^12.23.12",
    "html2canvas": "^1.4.1",
    "lucide-react": "^0.478.0",
    "motion-utils": "^12.23.6",
    "next": "14.2.31",
    "postcss": "^8",
    "react": "^18",
    "react-dom": "^18",
    "react-swipeable": "^7.0.2",
    "tailwindcss": "^3.3.0",
    "typescript": "^5"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "^1.13.13",
    "@types/mime": "^3.0.4",
    "eslint": "^8.57.1",
    "eslint-config-next": "^14.2.31"
  },
  "overrides": {
    "glob": "^10.4.5",
    "rimraf": "^6.0.1",
    "@humanwhocodes/config-array": "npm:@eslint/config-array@^0.19.1",
    "@humanwhocodes/object-schema": "npm:@eslint/object-schema@^2.1.5"
  }
